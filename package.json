{
  "name": "firstboilerplate",
  "version": "1.0.0",
  "description": "first boilerplate to ts",
  "main": "src/server/index.ts",
  "scripts": {
    "ts": "tsc --watch",
    "dev": "nodemon ./build/server/index.js"
  },
  "author": "Andrew Ortiz",
  "license": "ISC",
  "_moduleAliases": {
    "@contexts": "build/contexts",
    "@shared": "build/shared",
    "@server": "build/server"
  },
  "engines": {
    "node": ">=12.16.1",
    "npm": ">=6.13.4"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/compression": "^1.7.0",
    "@types/cors": "^2.8.6",
    "@types/express": "github:types/express",
    "@types/glob": "^7.1.1",
    "@types/helmet": "0.0.47",
    "@types/morgan": "^1.9.0",
    "@types/node": "^14.0.5",
    "@types/swagger-ui-express": "^4.1.2",
    "@types/uuid": "^8.0.0",
    "@types/uuid-validate": "0.0.1",
    "@types/yamljs": "^0.2.31",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.6",
    "nodemon": "^2.0.4",
    "tslint": "^6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.9.3"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "glob": "^7.1.6",
    "helmet": "^3.22.0",
    "http-status": "^1.4.2",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "swagger-ui-express": "^4.1.4",
    "uuid": "^8.1.0",
    "uuid-validate": "0.0.3",
    "winston": "^3.2.1",
    "yamljs": "^0.3.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{src}/**/*.ts": [
      "prettier --write",
      "tslint --fix",
      "git add"
    ]
  }
}
